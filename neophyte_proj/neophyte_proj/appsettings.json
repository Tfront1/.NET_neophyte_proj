{
  "Jwt": {
    "Issuer": "https://localhost:7022/",
    "Audience": "https://localhost:7022/"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log.txt"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
